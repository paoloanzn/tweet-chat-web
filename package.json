{
  "name": "tweet-chat-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:server \"dev:vite -- --host\"",
    "dev:vite": "vite",
    "dev:server": "cross-env NODE_ENV=development node --watch src/server/main.js",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean": "rimraf dist-client",
    "format": "prettier --write .",
    "test": "vitest",
    "test:watch": "vitest watch",
    "migrate": "node scripts/apply-migrations.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@fastify/cors": "^11.0.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/vite": "^4.1.5",
    "agent-twitter-client": "^0.0.18",
    "ai": "^4.3.15",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.15.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "tailwindcss": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdoc-to-markdown": "^9.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "supabase": "^2.22.12",
    "vite": "^6.3.5",
    "vitest": "^3.1.3"
  }
}
